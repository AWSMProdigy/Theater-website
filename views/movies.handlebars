<div class="container mt-4 mb-4">
    <h1>Featured Movies</h1>
    <div class="row flex-nowrap">
      {{#if loggedIn}}
        {{#each movieData as |movie|}}
        {{> movie-details-logged }}
        {{/each}}
      {{else}}
        {{#each movieData as |movie|}}
        {{> movie-details }}
        {{/each}}
      {{/if}}
    </div>
</div>

